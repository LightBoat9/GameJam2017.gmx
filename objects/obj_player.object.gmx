          <argument>
            <kind>1</kind>
            <string>///Player Movement

//Gravity
vsp += grav;

//Input
h_move = global.key_right - global.key_left;
hsp = h_move * MOVESPEED;

//Check is player is on the ground
if (place_meeting(x,y+1,obj_solid)) grounded = true;
else grounded = false;

if (grounded == true) &amp;&amp; (global.key_jump) vsp = -JUMPSPEED;

///Movement
for (i = 0; i &lt; abs(hsp); i++) {
    if (!place_meeting(x+sign(hsp),y,obj_solid)) {
        x += sign(hsp);
    }
    else hsp = 0;
}

for (i = 0; i &lt; abs(vsp); i++) {
    if (!place_meeting(x,y+sign(vsp),obj_solid)) {
        y += sign(vsp);
    }
    else vsp = 0;
}
</string>
